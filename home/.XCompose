include "/usr/share/X11/locale/en_US.UTF-8/Compose"

<Multi_key> <A> <A> : "∀"
<Multi_key> <E> <E> : "∃"

<Multi_key> <a> <n> <d> : "∧"
<Multi_key> <o> <r>     : "∨"

<Multi_key> <i> <n>          : "∈"
<Multi_key> <exclam> <i> <n> : "∉"
<Multi_key> <n> <i>          : "∋"
<Multi_key> <exclam> <n> <i> : "∌"

<Multi_key> <S> <parenleft>  : "⊂"
<Multi_key> <S> <parenright> : "⊃"
<Multi_key> <S> <equal> <parenleft> : "⊆"
<Multi_key> <S> <u> : "∪"
<Multi_key> <S> <n> : "∩"
<Multi_key> <braceleft> <braceright>  : "∅"

<Multi_key> <less> <minus>	 	: "←" U2190 # LEFTWARDS ARROW
<Multi_key> <minus> <greater> 		: "→" U2192 # RIGHTWARDS ARROW

<Multi_key> <t> <f> : "∴" U2234 # THEREFORE
<Multi_key> <b> <c> : "∵" U2235 # BECAUSE


